    "@babel/preset-env": "^7.0.0-beta.47",
    "babel-eslint": "^8.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.5",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
